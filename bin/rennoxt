#!/bin/bash

# Rename files preserving old file name extension

OLDNAME=$1
KEEPEXTENSION=$(echo $OLDNAME | awk -F '.' '{print $NF}')
NEWNAME=$(echo $2 | awk -F '.' -vOFS='.' '{NF='$KEEPEXTENSION';print}')
echo $OLDNAME $NEWNAME
#if [ "$OLDNAME" != "$NEWNAME" ]; then
#	mv "$OLDNAME" "$NEWNAME" &> /dev/null
#fi
